<joint name="lidar_rot" type="continuous">
  <origin rpy="0 0 0" xyz="0.0 0.0 0.1"/>
  <parent link="base_link"/>
  <child link="LIDAR_v3_1"/>
  <axis xyz="0.0 -0.0 1.0"/>
</joint>

